[
  {
    "name": "CSVToSQLDataFlow",
    "type": "ExecuteDataFlow",
    "dependsOn": [],
    "policy": {
      "timeout": "0.00:20:00",
      "retry": 0,
      "retryIntervalInSeconds": 30,
      "secureOutput": false,
      "secureInput": false
    },
    "userProperties": [],
    "typeProperties": {
      "dataflow": {
        "referenceName": "${data_flow_name}",
        "type": "DataFlowReference",
        "parameters": {
          "RankingYear": {
            "value": "@{pipeline().parameters.PipelineProcessingYear}",
            "type": "Expression"
          }
        },
        "datasetParameters": {
          "rawData": {
            "CSVFileName": {
              "value": "@{pipeline().parameters.PipelineProcessingYear}_rankings.csv",
              "type": "Expression"
            }
          }
        }
      },
      "compute": {
        "coreCount": 8,
        "computeType": "General"
      },
      "traceLevel": "Fine"
    }
  }
]
